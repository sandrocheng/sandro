<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ffcfdce4" android:endColor="#ffcfdce4" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffcfdce4" />
            <stroke android:width="0.5dip" android:color="#ffacacac" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ffcfdce4" android:endColor="#ffcfdce4" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffcfdce4" />
                <stroke android:width="0.5dip" android:color="#ffacacac" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ff83da11" android:endColor="#ff83da11" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff83da11" />
                <stroke android:width="0.5dip" android:color="#ffacacac" />
            </shape>
        </clip>
    </item>
</layer-list>