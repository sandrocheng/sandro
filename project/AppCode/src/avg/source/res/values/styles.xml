<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CodeFont" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textColor">#ff000000</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="CodeFontBold" parent="@style/CodeFont">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="TinyCodeFont" parent="@*android:style/TextAppearance.Small">
        <item name="android:textSize">10.0sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="BaseTitle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="AlertTitle" parent="@style/BaseTitle">
        <item name="android:textSize">20.0sp</item>
    </style>
    <style name="Dialog" parent="@*android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="boldText">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="NormalText">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="DialogSubtitleText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="DialogBodyText">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffbbbbbb</item>
    </style>
    <style name="ProductName">
        <item name="android:textSize">25.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="ListItemTitle">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text_color_white</item>
    </style>
    <style name="ListItemTitleDisable">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#99999999</item>
    </style>
    <style name="ListItemBody">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffbbbbbb</item>
    </style>
    <style name="ListItemBodyDisable" parent="@style/ListItemBody">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#99999999</item>
    </style>
    <style name="ListItemBodyLight" parent="@style/ListItemBody">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ListItemMoreInfo" parent="@style/ListItemBodyLight">
        <item name="android:textSize">13.0sp</item>
    </style>
    <style name="ListItemTitleMoreInfo">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/blue_note</item>
    </style>
    <style name="ListItemExpired" parent="@style/ListItemTitleMoreInfo">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="ListItemBodyDark" parent="@style/ListItemBody">
        <item name="android:textColor">@color/traffic_text</item>
    </style>
    <style name="Spinner">
        <item name="android:textColor">#ffff0000</item>
    </style>
    <style name="ProgressText" parent="@style/ListItemBody">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TitleBold">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff050505</item>
    </style>
    <style name="ListPadding">
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:paddingBottom">5.0dip</item>
    </style>
    <style name="BatteryStateListChildView">
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginTop">0.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
        <item name="android:layout_marginBottom">0.0dip</item>
    </style>
    <style name="BatteryListPadding" parent="@style/ListPadding">
        <item name="android:paddingLeft">0.0dip</item>
        <item name="android:paddingRight">0.0dip</item>
    </style>
    <style name="NormalTextGrey" parent="@style/NormalText">
        <item name="android:textColor">#ff686869</item>
    </style>
    <style name="BatteryLevelText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff323232</item>
    </style>
    <style name="NotificationText">
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>
    <style name="NotificationTitle">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>
    <style name="ActionbarText">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffbbbbbb</item>
    </style>
    <style name="TextWhiteLarge">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text_color_white</item>
    </style>
    <style name="TextBlueLarge" parent="@style/TextWhiteLarge">
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">#ff1ec7ff</item>
    </style>
    <style name="TextOrangeLargeItalic" parent="@style/TextWhiteLarge">
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">@color/orange_warning</item>
    </style>
    <style name="TextGreyMedium">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffbbbbbb</item>
        <item name="android:textColorLink">@color/blue_note</item>
    </style>
    <style name="TextGreySmall">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffbbbbbb</item>
    </style>
    <style name="TextGreyMediumBold" parent="@style/TextGreyMedium">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="TrialBigBlueText" parent="@style/TextWhiteLarge">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/blue_note</item>
    </style>
    <style name="UpgradeLink" parent="@style/TextWhiteLarge">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text_color_white_link</item>
    </style>
    <style name="TrialDaysLeftOrangeText" parent="@style/TextWhiteLarge">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/orange_warning</item>
    </style>
    <style name="TextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/primary_text_holo_dark</item>
        <item name="android:textColorHighlight">@color/highlighted_text_holo_dark</item>
        <item name="android:textColorHint">@color/hint_foreground_holo_dark</item>
        <item name="android:textColorLink">@color/blue_note</item>
    </style>
    <style name="TextAppearance.Holo" parent="@style/TextAppearance" />
    <style name="holo_edittext" parent="@*android:style/Widget.EditText">
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/edit_text_holo_dark</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="progress_bar_horizontal">
        <item name="android:maxHeight">25.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/progress_indeterminate_horizontal_holo</item>
        <item name="android:progressDrawable">@drawable/progress_horizontal_holo_dark</item>
        <item name="android:minHeight">25.0dip</item>
    </style>
    <style name="seekbar_bar_holo">
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:paddingRight">16.0dip</item>
        <item name="android:focusable">true</item>
        <item name="android:maxHeight">13.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:progressDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:minHeight">13.0dip</item>
        <item name="android:thumb">@drawable/scrubber_control_selector_holo</item>
        <item name="android:thumbOffset">16.0dip</item>
    </style>
    <style name="AlertDialogHolo">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:fullDark">@drawable/dialog_full_holo_dark</item>
        <item name="android:topDark">@drawable/dialog_top_holo_dark</item>
        <item name="android:centerDark">@drawable/dialog_middle_holo_dark</item>
        <item name="android:bottomDark">@drawable/dialog_bottom_holo_dark</item>
        <item name="android:fullBright">@drawable/dialog_full_holo_dark</item>
        <item name="android:topBright">@drawable/dialog_top_holo_dark</item>
        <item name="android:centerBright">@drawable/dialog_middle_holo_dark</item>
        <item name="android:bottomBright">@drawable/dialog_bottom_holo_dark</item>
        <item name="android:bottomMedium">@drawable/dialog_bottom_holo_dark</item>
        <item name="android:centerMedium">@drawable/dialog_middle_holo_dark</item>
    </style>
    <style name="ButtonBar">
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">0.0dip</item>
        <item name="android:paddingTop">0.0dip</item>
        <item name="android:paddingRight">0.0dip</item>
        <item name="android:paddingBottom">0.0dip</item>
        <item name="android:divider">?dividerVertical</item>
    </style>
    <style name="TabWidget">
        <item name="android:textAppearance">@style/TextAppearanceTabWidget</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:paddingTop">6.0dip</item>
        <item name="android:paddingBottom">6.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextAppearanceTabWidget">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/tab_indicator_text</item>
    </style>
    <style name="WidgetHoloTab">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/tab_indicator</item>
        <item name="android:paddingTop">8.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:divider">@drawable/list_divider_holo_dark</item>
        <item name="android:minWidth">80.0dip</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:tabStripLeft">@null</item>
        <item name="android:tabStripRight">@null</item>
        <item name="android:tabStripEnabled">false</item>
        <item name="android:measureWithLargestChild">true</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">8.0dip</item>
    </style>
    <style name="CheckBoxHolo">
        <item name="android:textAppearance">?android:textAppearance</item>
        <item name="android:textColor">?android:textColorPrimaryDisableOnly</item>
        <item name="android:gravity">left|center</item>
        <item name="android:background">@drawable/btn_check_label_background</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:button">@drawable/btn_check_holo_dark</item>
    </style>
    <style name="WidgetHoloButtonToggle">
        <item name="android:disabledAlpha">?android:disabledAlpha</item>
        <item name="android:textAppearance">@style/ButtonText</item>
        <item name="android:background">@drawable/btn_toggle_bg</item>
        <item name="android:minHeight">48.0dip</item>
    </style>
    <style name="NewBorderDialogTheme" parent="@*android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/progress_bg_holo_dark</item>
    </style>
    <style name="ListView">
        <item name="android:divider">@drawable/list_divider</item>
        <item name="android:dividerHeight">1.0px</item>
    </style>
    <style name="AppLandingButtonUpperText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/blue_note</item>
    </style>
    <style name="AppLandingButtonLowerText">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/orange_warning</item>
    </style>
    <style name="ButtonText">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ButtonTextBright" parent="@style/ButtonText">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ButtonTextBlue" parent="@style/ButtonText">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ff0000ff</item>
    </style>
    <style name="MyButton">
        <item name="android:textAppearance">@style/ButtonText</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@color/orange</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="ButtonHolo">
        <item name="android:textColor">@color/primary_text_holo_dark</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/btn_default_holo_dark</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:minWidth">64.0dip</item>
        <item name="android:minHeight">48.0dip</item>
    </style>
    <style name="ImageButtonHolo" parent="@style/ButtonHolo">
        <item name="android:minWidth">0.0dip</item>
        <item name="android:minHeight">0.0dip</item>
    </style>
    <style name="BlueButtonHolo" parent="@style/ButtonHolo">
        <item name="android:background">@drawable/btn_blue_holo_dark</item>
    </style>
    <style name="RadioButtonHolo">
        <item name="android:background">@drawable/btn_radio_label_background</item>
        <item name="android:button">@drawable/btn_radio_holo_dark</item>
    </style>
    <style name="SpinnerWidgetBlackText">
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="mytheme" parent="@*android:style/Theme">
        <item name="android:textAppearance">@style/TextAppearance.Holo</item>
        <item name="android:buttonStyle">@style/BlueButtonHolo</item>
        <item name="android:buttonStyleSmall">@style/ButtonHolo</item>
        <item name="android:buttonStyleInset">@style/ButtonHolo</item>
        <item name="android:windowBackground">@drawable/bg_blue</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:alertDialogStyle">@style/AlertDialogHolo</item>
        <item name="android:checkboxStyle">@style/CheckBoxHolo</item>
        <item name="android:editTextStyle">@style/holo_edittext</item>
        <item name="android:listViewStyle">@style/ListView</item>
        <item name="android:progressBarStyleHorizontal">@style/progress_bar_horizontal</item>
        <item name="android:seekBarStyle">@style/seekbar_bar_holo</item>
        <item name="android:radioButtonStyle">@style/RadioButtonHolo</item>
        <item name="android:tabWidgetStyle">@style/TabWidget</item>
        <item name="android:spinnerItemStyle">@style/SpinnerWidgetBlackText</item>
        <item name="android:textColor">@color/primary_text_holo_dark</item>
        <item name="android:listChoiceIndicatorSingle">@drawable/btn_radio_holo_dark</item>
        <item name="android:listChoiceIndicatorMultiple">@drawable/btn_radio_holo_dark</item>
        <item name="android:dialogTheme">@style/AlertDialogHolo</item>
        <item name="dividerHorizontal">@drawable/divider_vertical_holo_dark</item>
        <item name="dividerVertical">@drawable/divider_vertical_holo_dark</item>
        <item name="buttonBarStyle">@style/ButtonBar</item>
        <item name="buttonBarButtonStyle">@style/BlueButtonHolo</item>
    </style>
</resources>
