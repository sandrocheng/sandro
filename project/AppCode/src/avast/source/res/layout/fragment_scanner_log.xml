<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:background="@drawable/bg_title" android:layout_width="fill_parent" android:layout_height="@dimen/titlebar_height">
        <android.support.v4.app.FragmentBreadCrumbs android:id="@id/breadcrumbs" android:tag="breadcrumbs" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/content_marginHorizontal" />
    </LinearLayout>
    <LinearLayout android:background="@drawable/bg_menu_normal" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <ListView android:id="@*android:id/list" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_weight="1.0" android:footerDividersEnabled="false" style="@style/ListView.Transparent" />
        <RelativeLayout android:id="@id/empty" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="0.0">
            <ImageView android:id="@id/icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/content_marginHorizontal" android:layout_marginRight="@dimen/content_marginHorizontal" android:src="@drawable/ic_scanner_result_ok" android:layout_alignParentRight="true" />
            <TextView android:textSize="@dimen/font_medium" android:textColor="@color/text_ok" android:id="@id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/content_marginHorizontal" android:layout_marginTop="18.0dip" android:layout_marginRight="@dimen/content_marginHorizontal" android:text="@string/l_no_problems_found" android:layout_toLeftOf="@id/icon" android:layout_alignParentLeft="true" android:layout_alignParentTop="true" />
            <View android:id="@id/separator" android:background="@drawable/misc_menu_separator" android:layout_width="fill_parent" android:layout_height="2.0px" android:layout_marginTop="@dimen/content_marginHorizontal" android:layout_below="@id/title" android:layout_alignLeft="@id/title" android:layout_alignRight="@id/icon" />
            <TextView android:id="@id/date_label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/content_marginHorizontal" android:text="@string/l_last_date" android:layout_below="@id/separator" android:layout_alignLeft="@id/title" />
            <TextView android:id="@id/date_value" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="20. 7. 2010 10:30" android:layout_alignBaseline="@id/date_label" android:layout_alignRight="@id/icon" />
            <TextView android:id="@id/objects_label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/l_objects_scanned" android:layout_below="@id/date_label" android:layout_alignLeft="@id/title" />
            <TextView android:id="@id/objects_value" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="3500" android:layout_alignBaseline="@id/objects_label" android:layout_alignRight="@id/icon" />
            <TextView android:id="@id/time_label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/l_time_elapsed" android:layout_below="@id/objects_label" android:layout_alignLeft="@id/title" />
            <TextView android:id="@id/time_value" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="35:00" android:layout_alignBaseline="@id/time_label" android:layout_alignRight="@id/icon" />
            <Button android:id="@id/b_ok" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/content_marginHorizontal" android:text="@string/l_ok" android:layout_below="@id/time_label" android:layout_alignLeft="@id/title" android:layout_alignRight="@id/icon" style="@style/Button.Green" />
            <View android:layout_width="fill_parent" android:layout_height="@dimen/content_marginHorizontal" android:layout_below="@id/b_ok" />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>