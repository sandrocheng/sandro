<?xml version="1.0" encoding="utf-8"?>
<!-- checkbox：未选中为off，选中为on(对号) -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/sel_menu_setting_switch_on" android:state_selected="true"/>
    <item android:drawable="@drawable/sel_menu_setting_switch_on" android:state_checked="true" android:state_enabled="true"/>
    <item android:drawable="@drawable/sel_menu_setting_switch_disable" android:state_enabled="false" android:state_checked="true"/>
    <item android:drawable="@drawable/sel_menu_setting_switch_off_disable" android:state_enabled="false" android:state_checked="false"/>
    <item android:drawable="@drawable/sel_menu_setting_switch_off" android:state_checked="false" android:state_enabled="true"/>
    <item android:drawable="@drawable/sel_menu_setting_switch_off"/>
</selector>