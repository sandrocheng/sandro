<?xml version="1.0" encoding="utf-8"?>
<com.tencent.onesecurity.ui.customview.layout.booster.CleanRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.tencent.onesecurity"
    android:id="@+id/rl_clean"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/ll_clean_info"
        android:layout_width="fill_parent"
        android:layout_height="38.3dp"
        android:background="@drawable/bg_common_list"
        android:baselineAligned="false"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="16.7dp"
        android:paddingRight="16.7dp" >

        <TextView
            android:id="@+id/tv_scan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ellipsize="middle"
            android:singleLine="true"
            android:text="@string/booster_clean_scan"
            android:textColor="@color/blue_9_20558e"
            android:textSize="@dimen/textsize_13sp" />

        <TextView
            android:id="@+id/tv_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="@color/blue_9_20558e"
            android:textSize="@dimen/textsize_13sp" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/rl_btn_clean"
        android:layout_width="fill_parent"
        android:layout_height="83.3dp"
        android:layout_alignParentBottom="true"
        android:gravity="center_horizontal"
        android:orientation="vertical" >

        <com.tencent.onesecurity.ui.customview.widget.CustomIconButton
            android:id="@+id/manage_app_btn_clean"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="17dp"
            app:buttonIcon="@drawable/btn_booster_clean_nor"
            app:buttonIconDisabled="@drawable/btn_booster_clean_disabled"
            app:buttonIconPress="@drawable/btn_booster_clean_press"
            app:buttonText="@string/booster_btn_clean"
            app:buttonTextColor="@color/white_1_fafafa"
            app:buttonTextColorPress="@color/white_1_fafafa"
            app:buttonTextPadding="4"
            app:buttonType="icon_left" />
    </RelativeLayout>

